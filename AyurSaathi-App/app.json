{
  "expo": {
    "name": "AyurSaathi",
    "slug": "ayursaathi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1F1C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0D1F1C"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": ["expo-asset"]
  }
}
